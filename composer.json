{
    "name": "conlect/image-iiif",
    "description": "IIIF Image API 3.0",
    "keywords": [
        "iiif",
        "image api",
        "iiif-image"
    ],
    "homepage": "https://github.com/conlect/image-iiif",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremy Tubbs",
            "email": "jeremytubbs@gmail.com",
            "homepage": "https://jeremytubbs.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "hassankhan/config": "^3.1.0",
        "intervention/image": "^3.6.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.21.1",
        "larapack/dd": "^1.1",
        "phpunit/phpunit": "^10.3.2"
    },
    "suggest": {
        "ext-gd": "to use GD library based image processing.",
        "ext-imagick": "to use Imagick based image processing."
    },
    "autoload": {
        "psr-4": {
            "Conlect\\ImageIIIF\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Conlect\\ImageIIIF\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "@putenv XDEBUG_MODE=coverage",
            "vendor/bin/phpunit"
        ],
        "test-coverage": [
            "@putenv XDEBUG_MODE=coverage",
            "vendor/bin/phpunit --coverage-html coverage"
        ],
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
