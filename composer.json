{
    "name": "conlect/image-iiif",
    "description": "iiif image API 3.0",
    "keywords": [
        "conlect",
        "iiif",
        "image api"
    ],
    "homepage": "https://github.com/conlect/image-iiif",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremy Tubbs",
            "email": "jtubbs@conlect.io",
            "homepage": "https://conlect.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "hassankhan/config": "^3.0.0",
        "intervention/image": "^2.7.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "larapack/dd": "^1.1",
        "phpunit/phpunit": "^9.3"
    },
    "suggest": {
        "ext-gd": "to use GD library based image processing.",
        "ext-imagick": "to use Imagick based image processing.",
        "intervention/imagecache": "Caching extension for the Intervention Image library"
    },
    "autoload": {
        "psr-4": {
            "Conlect\\ImageIIIF\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Conlect\\ImageIIIF\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": [
            "@putenv XDEBUG_MODE=coverage",
            "vendor/bin/phpunit --coverage-html coverage"
        ],
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"

    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
